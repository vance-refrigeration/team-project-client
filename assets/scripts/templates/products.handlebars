{{#each product as |product|}}

<div class="col-md-4">
  <div class="panel panel-default">
    <div class="panel-heading"><h3>{{product.name}}</h3></div>
    <div class="panel-body">
        <p>{{product.description}}</p>
        <p>{{price}}</p>
      <button data-id="{{product.id}}" class="btn btn-default" id="add-to-cart">Add To Cart</button>
    </div>
  </div>
</div>
